{
  "version": 2,
  "functions": {
    "api/souls/[id]/download.js": {
      "includeFiles": "souls/**/*.md"
    }
  },
  "routes": [
    {
      "src": "/api/mcp/manifest",
      "dest": "/api/mcp/manifest.js"
    },
    {
      "src": "/api/mcp/tools/(.*)",
      "dest": "/api/mcp/tools/$1.js"
    },
    {
      "src": "/api/souls/([^/]+)/download",
      "dest": "/api/souls/[id]/download.js?id=$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/soul",
      "dest": "/soul.html"
    },
    {
      "src": "/WEBMCP\\.md",
      "dest": "/api/mcp/webmcp_markdown.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
